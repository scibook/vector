{
    "name": "@scibook/vector",
    "version": "1.0.0",
    "description": "A vector library that supports 2,3,4 and N-dimensional vectors.",
    "source": "src/index.ts",
    "main": "./dist/main.js",
    "module": "./dist/module.js",
    "types": "dist/types.d.ts",
    "scripts": {
        "clean:deep": "npm run clean && npm run clean:alldirs && npm i",
        "clean:alldirs": "rimraf --glob ./.parcel-cache/** ./docs/** ./coverage/** ./node_modules/** ./dist/**",
        "build": "rimraf --glob dist/** && npm run fmt && npm run typecheck && npx parcel build",
        "lint": "npx eslint ./src/**",
        "fmt": "npx prettier ./src/** ./*.json ./*.js --write",
        "test": "jest",
        "test-ci": "jest --ci",
        "makebadges": "npx istanbul-badges-readme",
        "typecheck": "tsc --project tsconfig.json",
        "makedocs": "rimraf --glob docs/** && npx typedoc --out docs ./src/index.ts",
        "publishdocs": "npm run makedocs && npx gh-pages -d ./docs"
    },
    "files": [
        "dist"
    ],
    "keywords": [
        "linear algebra",
        "math",
        "vector"
    ],
    "author": "Lina Adkins",
    "license": "MIT",
    "devDependencies": {
        "@parcel/compressor-gzip": "^2.12.0",
        "@parcel/packager-ts": "^2.12.0",
        "@parcel/transformer-typescript-types": "^2.12.0",
        "@types/jest": "^29.5.12",
        "eslint": "^8.57.0",
        "eslint-plugin-tsdoc": "^0.2.17",
        "gh-pages": "^6.1.1",
        "istanbul-badges-readme": "^1.8.5",
        "jest": "^29.7.0",
        "parcel": "^2.12.0",
        "prettier": "3.2.5",
        "rimraf": "^5.0.5",
        "ts-jest": "^29.1.2",
        "ts-loader": "^9.5.1",
        "typedoc": "^0.25.11",
        "typescript": "^5.3.3",
        "typescript-eslint": "^7.1.0"
    }
}
